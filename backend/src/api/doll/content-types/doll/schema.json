    },
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "images": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": true,
      "required": true
    },
    "price": {
      "type": "integer",
      "required": true
    },
    "workDone": {
      "type": "richtext",
      "required": true
    },
    "includedItems": {
      "type": "richtext",
      "required": true
    },
    "slug": {
      "type": "uid",
      "required": true,
      "targetField": "name"
    }
  }
}
