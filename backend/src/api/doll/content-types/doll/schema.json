{
  "kind": "collectionType",
  "collectionName": "dolls",
  "info": {
    "singularName": "doll",
    "pluralName": "dolls",
    "displayName": "doll"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "isSold": {
      "type": "boolean",
